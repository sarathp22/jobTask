    <div class="container ">
        <p class="text-center" style="color: darkmagenta;font-size: 30px;margin-top: 100px;">Student Registration</p>


      <div class="row">
        <div class="col-sm-12 col-md-6 mx-auto bor" >
          <h1 class="text-center text-primary">Student Edit</h1>
          <form action="/studentEdit" method="POST" id="form1">
            
                
                <input type="hidden" class="form-control col-sm-12 col-md-8" name="id" aria-describedby="studId" placeholder="Enter Student Name" value="<%= data._id %>">
          
            <div class="form-group row">
              <label for="studName" class="col-sm-12 col-md-3">Student Name</label>
              <input type="text" class="form-control col-sm-12 col-md-8" name="studName" aria-describedby="studId" placeholder="Enter Student Name" value="<%= data.studName %>" required>
            </div>
            <div class="form-group row">
              <label for="regNumber" class="col-sm-12 col-md-3">enter register number</label>
              <input type="text" class="form-control col-sm-12 col-md-8" name="regNumber" placeholder="Enter register number" value="<%= data.regNumber %>" required>
            </div>
            <div class="form-group row">
                <label for="sub1" class="col-sm-12 col-md-3">enter subject 1 Mark</label>
                <input type="number" class="form-control col-sm-12 col-md-8" name="sub1" placeholder="Enter subject 1 Mark" value="<%= data.sub1 %>" required>
              </div>
              <div class="form-group row">
                <label for="sub2" class="col-sm-12 col-md-3">enter subject 2 Mark</label>
                <input type="number" class="form-control col-sm-12 col-md-8" name="sub2" placeholder="Enter subject 2 Mark" value="<%= data.sub2 %>" required>
              </div>
              <div class="form-group row">
                <label for="sub3" class="col-sm-12 col-md-3">enter subject 3 Mark</label>
                <input type="number" class="form-control col-sm-12 col-md-8" name="sub3" placeholder="Enter subject 3 Mark" value="<%= data.sub3 %>"  required>
              </div>
              <div class="form-group row">
                <label for="total" class="col-sm-12 col-md-3">enter total Mark</label>
                <input type="number" class="form-control col-sm-12 col-md-8" name="total" placeholder="Enter total Mark" value="<%= data.total %>" required>
              </div>
           
            <button type="submit" class="btn btn-primary  offset-3">Submit</button>
        
          </form>
        </div>
        
      </div>
      </div>

      </body>
</html>